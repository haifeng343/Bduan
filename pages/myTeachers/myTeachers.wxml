    <view class="container">
      <view class="touch-item {{item.isTouchMove ? 'touch-move-active' : ''}}" data-index="{{index}}" bindtouchstart="touchstart" bindtouchmove="touchmove" wx:for="{{items}}" wx:key="">
        <view class="content between">
          <view class='column'>
            <view class='start'>
              <image src='../../img/avator.png' class='avator'></image>
              <view class='column mt35'>
                <view class='between'>
                  <view class='start'>
                    <view class='ft28 bold'>Apri</view>
                    <view class='ml20 ft24 cc'>旅游英语 | 5年教龄</view>
                  </view>
              </view>
              <view class='between ft26'>
