<view class='noHas' wx:if="{{items.length<=0}}">
  <image src='../../img/zanwu.png' mode='widthFix' class='noList'></image>
  <!-- <view class='zanwu'>暂无数据</view> -->
</view>
<view wx:else>
  <view class="container">
    <view class='column item pb20' wx:for="{{items}}" wx:key>
      <view class='start'>
        <view>
          <image src='{{item.HeadUrl}}' class='avator' mode='aspectFill'></image>
        </view>
        <view class='column'>
          <view class='between'>
            <view class='start'>
              <view class='ft28 bold'>{{item.Name}}</view>
              <view class='ml20 ft24 cc'>{{item.Titles}} | {{item.TeachingAge}}教龄</view>
            </view>
          </view>
          <view class='between ft28'>
            <view class='cc'>{{item.Honor}}</view>
          </view>
        </view>
        <image class='edit' bindtap='edit' data-id="{{item.Id}}" src='../../img/more.png' mode='widthFix' lazy-load='true'></image>
      </view>
    </view>
  </view>
  <view class='add' wx:if="{{IsAdministrator}}" bindtap='bindAddTeacher'>+添加老师</view>
</view>