page {
  background-color: white;
}

.container {
  height: auto;
}

.address{
  width: 100%;
  background-color: #fff;
  height: 100vh;
}
.noads{
  width: 450rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 auto;
}
.nohave{
  font-size: 32rpx;
  text-align: center;
  line-height: 50rpx;
}
.haveto{
  font-size: 28rpx;
  text-align: center;
}
.List{
  width: 100%;
  height: 100vh;
  padding: 0 0 0 5%;
  box-sizing: border-box;
  overflow-y: scroll;
  padding-bottom: 100rpx;
}
.item{
  width: 100%;
  padding: 25rpx 0;
  border-bottom:1rpx solid #F7F7F7;
  margin-top: 0;
}
.item:last-child{
  border: none;
}
.name{
  font-size: 32rpx;
}
.addressDetail{
  font-size: 26rpx;
  color: #A19B9B;
}
.avator{
  width: 135rpx;
  height: 135rpx;
  border-radius: 50%;
  margin-right: 15rpx;
}
.column{
  position: relative;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}
.between{
  width:540rpx;
}
.edit{
  display: flex;
  align-items: center;
  width: 34rpx;
  height: 34rpx;
}