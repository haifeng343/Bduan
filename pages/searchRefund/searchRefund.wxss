.startd {
  width: 100%;
  height: 100rpx;
  padding: 0 2.5%;
  box-sizing: border-box;
  background-color: #f2f2f2;
  position: fixed;
  top: 0;
  padding-top: 20rpx;
  z-index: 22;
}
.clearDiv{
  width: 60rpx;
  height: 60rpx;
  position: fixed;
  right: 14%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
  z-index: 13;
}
.absokl{
  position: absolute;
  right: 15rpx;
  top: 15rpx;
}
.cleard {
  width: 28rpx;
  height: 28rpx;
  z-index: 14;
}

.searchInput {
  width: 88%;
  height: 60rpx;
  padding-right: 7%;
  font-size: 28rpx;
  line-height: 60rpx;
  background-color: #fff;
  border-radius: 30rpx;
  padding-left: 10%;
  box-sizing: border-box;
  text-align: left;
  background-image: url('data:image/png;base64,');
  background-size: 36rpx 36rpx;
  background-repeat: no-repeat;
  background-position: 6% 50%;
}

.searchRight {
  width: 12%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: right;
  z-index: 2;
}

.poab {
  width: 220rpx;
  display: flex;
  justify-content: center;
  position: absolute;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  z-index: 2;
  border-right-color: #fff;
}

.poab::after {
  content: "";
  width: 2rpx;
  height: 35rpx;
  background-color: #e7e7e7;
  margin-left: 5rpx;
}

.selectOption {
  position: fixed;
  width: 200rpx;
  height: 120rpx;
  font-size: 28rpx;
  padding: 10rpx 0;
  color: #000;
  background-color: #fff;
  border-radius: 15rpx;
  line-height: 60rpx;
  top: 80rpx;
  left:2%;
  text-align: center;
  box-shadow:0 0 15rpx #e9e9e9;
  z-index: 2;
}

.List {
  width: 100%;
  height: auto;
  background-color: #fff;
  margin-top: 100rpx;
}

.item {
  width: 95%;
  height: auto;
  margin-left: 5%;
  border-bottom: 1rpx solid #f3f3f3;
  padding: 25rpx 5% 25rpx 0;
  box-sizing: border-box;
  position: relative;
}

.ft26 {
  font-size: 28rpx;
}

.icon {
  margin-top: 0;
}

.item .start {
  line-height: 55rpx;
  display: flex;
  align-items: center;
}

.item .wrap {
  height: 35rpx;
  line-height: 35rpx;
  font-size: 28rpx;
  padding-bottom: 40rpx;
}

.List1 .item {
  width: 100%;
  height: auto;
  padding-left: 5%;
  margin-left: 0;
}

.historyTop {
  margin-top: 100rpx;
}

.histitle {
  font-size: 30rpx;
  margin-bottom: 10rpx;
}

.delete {
  width: 45rpx;
  height: 45rpx;
}
.searchHistory{
  width: 100%;
  box-sizing: border-box;
  padding:  0 5%;
}
.historyList {
  padding: 0;
  max-height: 300rpx;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.historyItem {
  font-size: 28rpx;
  padding: 5rpx 15rpx;
  border-radius: 4rpx;
  background-color: #c3c3c3;
  color: #7d7d7d;
  margin-right: 30rpx;
  margin-top: 25rpx;
}
