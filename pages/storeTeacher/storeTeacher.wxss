
.item{
  width: 100%;
  height: auto;
  border-bottom: 1rpx solid #F7F7F7;
}
.item:last-child{
  border-color: transparent;
}
.avator{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}
.zan-dialog__container{
  width: 95%;
  height: 850rpx;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom:0;
  margin: auto;
}
.zan-content{
  height: 850rpx;
}
.know{
  position: absolute;
  width: 100%;
  bottom: 0;
  
}
.cancel{
  border-right: 1rpx solid #F1F1F1;
  width: 50%;
  box-sizing: border-box;
}
.cancel:last-child{
  border:none;
}
.dialogList{
  width: 100%;
  max-height: 400rpx;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
}
.dialogItem{
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #F7F7F7;
}
.dialogItem:last-child{
  border-color: transparent;
}
.dialogItem .chebox{
  margin-right: 15rpx;
}
