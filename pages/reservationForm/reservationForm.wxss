.container {
  border-radius: 15rpx;
  padding-top: 20rpx;
}

.containerBot {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 20rpx 5%;
  display: flex;
  flex-direction: column;
}

.calendar--calendar-header {
  width: 50%;
  justify-content: center!important;
  margin: 0 auto!important;
}
.container .timeAbsolute{
  position: absolute;
  top: 45rpx;
  left: 7%;
  font-size: 28rpx;
  color: #c1c1c1;
}
.modalList{
  width: 100%;
  height: auto;
  max-height: 666rpx;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
}
.modalFormList{
  width: 100%;
  height: auto;
  flex-direction: column;
}
.modalFormItem{
  width: 100%;
  display: flex;
  justify-content: flex-start;
  padding-top: 10rpx;
}
.modalFormItemRight{
  display: flex;
  line-height: 40rpx;
  flex-direction: column;
}
.checkAll{
  padding: 20rpx 0;
}
 .timeAbsolute1{
  position: absolute;
  top: 45rpx;
  right: 7%;
  font-size: 28rpx;
  color: #c1c1c1;
}

checkbox .wx-checkbox-input {
  height: 40rpx;
  width: 40rpx;
  font-size: 0;
  border-radius: 0!important;
  border: none;
  background: url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  height: 40rpx;
  width: 40rpx;
  border: none;
  border-radius: 0!important;
  font-size: 0;
  transform: translate(-50%, -50%) scale(1);
  background: url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
