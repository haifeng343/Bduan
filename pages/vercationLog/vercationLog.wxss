page{
  background-color: #fff;
}
.content{
  padding: 0;
}
.content1{
  height: 80rpx!important;
  line-height: 80rpx!important;
}
.start{
  padding: 0 5%;
  box-sizing: border-box;
}
.go{
  margin-left: 10rpx;
}
.header{
  position: fixed;
  top: 0;
  background-color: #F2F2F2;
  z-index: 9999;
}
.select{
  width: 30rpx;
  height: 30rpx;
}
.mtml{
  width: 100%;
  height: 100vh;
  position: absolute;
  top: 80rpx;
}
.item{
  flex: 1;
  
}
.flex{
  display: flex;
  flex: 1;
}
.contents{
  display: flex;
  justify-content: space-around;
  text-align: center;
  width: 100%;
  height: auto;
  flex-wrap: wrap;
  box-sizing: border-box;
  padding: 15rpx 0;
  border-bottom:1rpx solid #f4f4f4;
}
.contents:last-child{
  border: none;
}
.contents .item{
  line-height: 50rpx;
}
.kl{
  flex: 1;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}
.h80{
  height: 80rpx!important;
  line-height: 80rpx!important;
}
.contents .b1{
  margin-right: 5%;
  width: 33%;
  box-sizing: border-box;
}
.contents .b2{
  width: 33%;
  box-sizing: border-box;
}
.contents .b3{
  margin-left: 5%;
  width: 33%;
  box-sizing: border-box;
}
.item{
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 2.5%;
  display: flex;
  flex: 1;
  text-align: center;
  justify-content: center;
}
.List .item1{
  width: 33.33%;
  flex: 1;
  display: flex;
  justify-content: center;
}



