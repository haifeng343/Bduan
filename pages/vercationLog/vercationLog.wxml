<view class='start h80 content ft28 bort borb header'>
  <picker mode="multiSelector" value="{{date2}}" range='{{array}}' start="{{dataStart}}" end="{{dataEnd}}" fields="month" bindchange="bindDateChange">
    <view class="picker">
      {{date?date:now}}
    </view>
  </picker>
  <image src='../../img/select.png' class='select' mode='widthFix' lazy-load='true'></image>
</view>
<view class='nohasLog' wx:if="{{List.length<=0}}">
  <image src='../../img/zanwu.png' mode='widthFix' class='noImg'></image>
  <view class='notext'>暂无验证记录</view>
</view>
<view class='mtml' wx:else>
  <view class='content between ft28'>
    <!-- <view class='h100 ft28 bold'>券码</view> -->
    <view class='item'>购买账户</view>
    <view class='item'>项目名称</view>
    <view class='item'>验证时间</view>
  </view>
  <view class='content ft24' wx:for="{{List}}" wx:key>
    <view class='item between bord bort h100'>
      <!-- <view>{{item.TicketNumber}}</view> -->
      <view class='b1'>{{item.AccountName}}({{item.AccountMobile}})</view>
      <view class='b2'>{{item.ItemName}}({{item.StoreName}})</view>
      <view class='b3'>{{item.CheckTime}}</view>
    </view>
  </view>
</view>