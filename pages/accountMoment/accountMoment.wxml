<view class='address'>
  <view class='noHas' wx:if="{{List.length<=0}}">
    <image src='../../img/zanwu.png' mode='widthFix' class='noList'></image>
  </view>
  <view wx:else>
    <view class="container">
      <view class='column item pb20' wx:for="{{items}}" wx:key>
        <view class='start'>
          <image src='{{item.HeadUrl}}' mode='aspectFill' class='avator'></image>
          <view class='column'>
            <view class='between ft28 mt15'>
              <view>账户名: {{item.UserName}}</view>
              <view wx:if="{{item.IsAdministrator==false}}">普通账户</view>
              <view wx:else>管理员</view>
            </view>
            <view class='between ft24'>
              <view>手机号: {{item.Mobile}}</view>
              <!-- <view class='cc'>创建时间: 2019-12-12</view> -->
            </view>
          </view>
          <image class='edit' bindtap='edit' data-id="{{item.AccountId}}" src='../../img/more.png' mode='widthFix' lazy-load='true'></image>
        </view>
      </view>
    </view>

  </view>
  <view class='add' data-ids="{{ids}}" wx:if="{{IsAdministrator}}" bindtap='bindAddAccount'>新增账户</view>
</view>