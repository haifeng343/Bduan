.container{
  width: 95%;
  height: auto;
  display: flex;
  flex-direction: column;
  border-radius: 15rpx;
  background-color: #fff;
  margin: 20rpx auto;
}
.classItem{
  width:90%;
  box-sizing: border-box;
  margin: 0 5%;
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
  justify-content: space-between;
  flex-wrap: nowrap;
  position: relative;
  border-bottom: 1rpx solid #f4f4f4;
}
.w100{
  width: 100%;
}
.picker{
  width: 80%;
}
.btnSure{
  width: 95%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  color: #fff;
  background-color: #29D9D6;
  border-radius: 15rpx;
  margin: 0 auto;
  text-align: center;
}
label{
  width: 210rpx!important;
}
.itemRight,.itemRight1{
  width: 92%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: left;
  font-size: 28rpx;
}
.Gray{
  color: #f4f4f4!important;
}
.InputIcon {
  position: absolute;
  width: 48rpx;
  height: 48rpx;
  transform: rotate(-90deg);
  right: 0;
  top: 16rpx;
}
.itemRight1{
  width: 80%;
  text-align: left;
}