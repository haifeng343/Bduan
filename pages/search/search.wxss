.startd{
  width: 100%;
  padding: 0 2.5%;
  box-sizing: border-box;
  position: relative;
  margin-top: 20rpx;
}
.searchInput{
  width: 88%;
  height: 60rpx;
  font-size: 28rpx;
  line-height: 60rpx;
  background-color: #fff;
  border-radius: 30rpx;
  padding-left: 40%;
  box-sizing: border-box;
  padding-right: 20rpx;
  text-align: left;
  background-image: url('data:image/png;base64,');
  background-size: 36rpx 36rpx;
  background-repeat: no-repeat;
  background-position: 42% 50%;
}
.searchRight{
  width: 12%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: right;
}
.poab{
  width: 220rpx;
  display: flex;
  justify-content: center;
  position: absolute;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  z-index: 1;
  border-right-color: #fff;
}
.poab::after{
  content: "";
  width: 2rpx;
  height: 35rpx;
  background-color: #E7E7E7;
  margin-left: 5rpx;
}
.selectOption{
  position: absolute;
  width: 200rpx;
  height: 90rpx;
  font-size: 26rpx;
  padding: 10rpx 0;
  color: #949494;
  background-color: #fff;
  border-radius: 15rpx;
  line-height: 45rpx;
  top: 68rpx;
  text-align: center;
  box-shadow: 0 0 10rpx #f4f4f4;
}












